{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": [1, { "allow": ["_id", "_page", "_browser", "_pageNumber", "_articleList", "_isProductCard", "_articlesCount", "_updateArticlesList", "_searchNumbersPosition", "_repeatCount"] }]
    }
}
